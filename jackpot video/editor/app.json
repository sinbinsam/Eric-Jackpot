	"scripts": {
		"77F59395-AE07-4530-9F3B-9F2A974A3456": [
			{
				"name": "text",
				"source": "function update( event ) {\nvar loader = new THREE.FontLoader();\nvar shading_type = THREE.SmoothShading;\n\nloader.load( 'https://raw.githubusercontent.com/mrdoob/three.js/dev/examples/fonts/gentilis_regular.typeface.json', function ( font ) {\n\n\tvar geometry = new THREE.TextGeometry( '1000', {\n\t\tfont: font,\n\t\tsize: 2,\n\t\theight: .1,\n\t\tdepth: 1,\n\t\tcurveSegments: 12,\n\t\tbevelEnabled: true,\n\t\tbevelThickness: .01,\n\t\tbevelSize: .1,\n\t\tbevelOffset: 0,\n\t\tbevelSegments: 5\n\t} );\n    geometry.computeBoundingBox();\n\tgeometry.computeVertexNormals();\n\n\n    textGeo = new THREE.BufferGeometry().fromGeometry( geometry );\n\t\n\tvar centerOffset = - 0.5 * ( textGeo.boundingBox.max.x - textGeo.boundingBox.min.x );\n\n            var material = new THREE.MeshLambertMaterial({\n                color: 0xF3F3F3,\n                shading: shading_type,\n                transparent: true\n            });\n\nvar mesh = new THREE.Mesh( textGeo, material ) ;\n\t\n\tmesh.position.x = centerOffset\n\nscene.add(mesh)\n} );\n}"
			}],
